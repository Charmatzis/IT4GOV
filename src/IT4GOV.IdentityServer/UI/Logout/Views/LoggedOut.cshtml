@model IT4GOV.IdentityServer.UI.Logout.LoggedOutViewModel

<div class="page-header">
    <h1>
        Αποσύνδεση
        <small>Έχετε αποσυνδεθεί</small>
    </h1>

    <div hide-if-null="Model.ReturnInfo">
        Κάντε κλικ <a href="@Model.ReturnInfo.Uri">εδώ</a> για να επιστρέψετε πίσω στην 
        <span>@Model.ClientName</span> εφαρμογή.
    </div>

    @foreach (var url in Model.SignOutIFrameUrls)
    {
        <iframe class="signout" src="@url"></iframe>
    }
</div>
